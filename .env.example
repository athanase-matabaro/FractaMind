# FractaMind Environment Configuration
# Copy this file to .env and customize as needed

# Feature Flags
# Set to 'false' to disable workspace feature
VITE_FEATURE_WORKSPACE=true

# AI Configuration
# Options: 'live' (use Chrome Built-in AI) | 'mock' (use deterministic fallbacks)
# Mock mode is useful for testing, development without AI, or when API is unavailable
VITE_AI_MODE=live

# AI operation timeout in milliseconds (default: 15000 = 15 seconds)
# Increase if AI operations consistently timeout on slow devices
# Decrease for faster failure detection in production
VITE_AI_TIMEOUT_MS=15000

# Development
VITE_ENABLE_DEBUG_LOGS=false

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=false
